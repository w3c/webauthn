{
    "version": "0.2",
    "ignorePaths": [
        "/.cspell/*"
    ],
    "dictionaryDefinitions": [
        {
            "name": "names",
            "path": "./.cspell/names.txt",
            "addWords": true
        },
        {
            "name": "acronyms",
            "path": "./.cspell/acronyms.txt",
            "addWords": true
        },
        {
            "name": "misc",
            "path": "./.cspell/misc.txt",
            "addWords": true
        }
    ],
    "dictionaries": [
        "names",
        "acronyms",
        "misc"
    ],
    "words": [],
    "ignoreWords": [],
    "import": [],
    "ignoreRegExpList": [
        "/\\{#([^}]*)\\}/g",
        "/\\s*\\[\\[([^\\]]*)\\]\\]\\s*/g",
        "/\\(\\#([^\\s#][^)]*[^\\s)])\\)/g",
        "/&[^;\\s]+;/g",
        "/\\s*\\[=([^=]*)=\\]\\s*/g",
        "/class\\s*=\\s*['\"][^'\"]*['\"]/g",
        "/id\\s*=\\s*['\"][^'\"]*['\"]/g"
    ]
}