                },
                "opera": [
                    {
                        "version_added": "16",
                        "flags": [
                            {
                                "type": "runtime_flag",
                                "name": "--enable-experimental-webkit-features"
                            },
                            {
                                "type": "runtime_flag",
                                "name": "--enable-viewport"
                            }
                        ]
                    },
                    {
                        "prefix": "-o-",
                        "version_added": "11.1",
                        "version_removed": "15"
                    }
                ],
                "opera_android": [
                    {
                        "version_added": "16",
                        "flags": [
                            {
                                "type": "runtime_flag",
                                "name": "--enable-experimental-webkit-features"
